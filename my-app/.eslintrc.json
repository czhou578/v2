{
  "extends": "next/core-web-vitals",
  "rules": {
    "semi": ["warn", "always"],
    "quotes": ["error", "single"],
    "curly": "error",
    "array-bracket-newline": "error",
    "arrow-spacing": "error",
    "camelcase": "error",
    "comma-style":"warn",
    "eqeqeq": "error",
    "no-extra-semi": "error",
    "prefer-const": "error",
    "no-var": "error",
    "prefer-destructuring": "error"
  }
}
